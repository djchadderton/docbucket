<div id="<%= dom_id post %>">
  <p>
    <strong>User:</strong>
    <%= post.user.name %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>

  <p>
    <strong>Folder:</strong>
    <%= post.folder.name %>
  </p>

</div>
